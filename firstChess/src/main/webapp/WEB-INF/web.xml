<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <servlet>
  <servlet-name>serv1</servlet-name>
  <servlet-class>firstChess.chessLoginServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>serv2</servlet-name>
  <servlet-class>firstChess.chessSignUpServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>serv3</servlet-name>
  <servlet-class>firstChess.trueLogin</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>serv1</servlet-name>
  <url-pattern>/chessLoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>serv2</servlet-name>
  <url-pattern>/chessSignUpServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>serv3</servlet-name>
  <url-pattern>/trueLogin</url-pattern>
  </servlet-mapping>
  
</web-app>